{% extends 'base.j2' %}


{% block content %}
{% block list_description %}{% endblock %}
<ol>
	{% for res in resource.node.walk_resources_sorted_by_time() %}
	<li>
		<em>{{ res.meta.created }}</em><br>
		<a href="{{ content_url(res.url)}}">{{ res.meta.title }}</a>
	</li>
	{% endfor %}
</ol>
{% endblock %}